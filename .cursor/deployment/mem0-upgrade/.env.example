# .env для Mem0 OSS + Qdrant
# Скопируй в .env и заполни реальными значениями
# НИКОГДА не коммить .env с реальными ключами в git

# OpenAI — через Sanitizer Proxy (порт 8888)
# Запросы идут: Mem0 → Sanitizer Proxy → OpenAI API
OPENAI_API_KEY=sk-REPLACE_WITH_REAL_KEY

# Qdrant — адрес внутри Docker-сети
MEM0_VECTOR_STORE_PROVIDER=qdrant
MEM0_VECTOR_STORE_HOST=qdrant
MEM0_VECTOR_STORE_PORT=6333
MEM0_VECTOR_STORE_COLLECTION_NAME=openclaw-mem0

# Embeddings — через Sanitizer Proxy
MEM0_EMBEDDER_PROVIDER=openai
MEM0_EMBEDDER_MODEL=text-embedding-3-small
MEM0_EMBEDDER_BASE_URL=http://host.docker.internal:8888/v1

# LLM для extraction — через Sanitizer Proxy (дешевле gpt-4o)
MEM0_LLM_PROVIDER=openai
MEM0_LLM_MODEL=gpt-4o-mini
MEM0_LLM_BASE_URL=http://host.docker.internal:8888/v1
